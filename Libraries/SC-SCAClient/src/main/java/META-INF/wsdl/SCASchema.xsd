<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://xml.smilecoms.com/schema/SCA"
            elementFormDefault="qualified"  xmlns:tns="http://xml.smilecoms.com/schema/SCA">
    <!--
    #################################################
    Customer Management
    #################################################
    -->

    <!--
    Element Name  :  CustomerQuery
    Description  :  CustomerQuery is used whenever there is a need to lookup/search a customer on the system.
                     Currently, customers can be searched using their IMPU or Name. This element will be extended
                     by including other fields to search on.
    Platform/s Used: BM
    -->
    <xsd:complexType name="CustomerQuery">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="CustomerId" type="xsd:int" minOccurs="0" />
                    <xsd:element name="LastName" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="FirstName" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="IdentityNumber" type="xsd:string" minOccurs="0"/>                
                    <xsd:element name="NationalIdentityNumber" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="IsNinVerified" type="xsd:string"/>  
                    <xsd:element name="SecurityGroupType" type="xsd:string"/>              
                    <xsd:element name="SSOIdentity" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="OrganisationName" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AlternativeContact" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EmailAddress" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ResultLimit" type="xsd:int"/>
                    <xsd:element name="Offset" minOccurs="0" type="xsd:int"/>
                    <xsd:element name="ProductInstanceResultLimit" type="xsd:int" minOccurs="0" />
                    <xsd:element name="ProductInstanceOffset" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="KYCStatus" type="xsd:string" minOccurs="0"/>                    
                    <xsd:element name="Verbosity" type="tns:stCustomerLookupVerbosity" minOccurs="0"/>
                    <xsd:element name="CountyCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="MandatoryKYCFields" type="tns:MandatoryKYCFields" minOccurs="0"/>
                    <xsd:element name="CustomerNinData" type="tns:CustomerNinData" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="TenantQuery">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="CustomerId" minOccurs="0" type="xsd:int"/>
                    <xsd:element name="SSOIdentity" minOccurs="0" type="xsd:string"/>
                    <xsd:element name="EmailAddress" minOccurs="0" type="xsd:string"/>
                </xsd:sequence>       
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="TenantData">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="tenant" type="xsd:string"/>
                </xsd:sequence>       
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>


    <xsd:element name="TenantQuery" type="tns:TenantQuery"/>
    <xsd:element name="TenantData" type="tns:TenantData"/>
    
    
    <xsd:complexType name="CustomerCommunicationData">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>                    
                    <xsd:element name="AccountId" type="xsd:long" minOccurs="0"/>
                    <xsd:element name="CustomerId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="ServiceInstanceId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="ProductInstanceId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="SSOIdentity" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SubjectResourceName" type="xsd:string"/>
                    <xsd:element name="SubjectParameters" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                    <xsd:element name="BodyResourceName" type="xsd:string"/>
                    <xsd:element name="BodyParameters" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                    <xsd:element name="BodyXML" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CCAddress" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="BCCAddress" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AttachmentResourceName" type="xsd:string"/>
                    <xsd:element name="AttachmentXML" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AttachmentFileName" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AttachmentStorageLocation" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AttachmentBase64" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Blocking" type="xsd:boolean"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="CustomerCommunicationData" type="tns:CustomerCommunicationData"/>
    <!--
    Element Name   : CustomerList
    Description    : This element is used to return the results of a customer lookup request.
                     This element contains a list of customers found.
    Platform/s Used: BM
    -->
    <xsd:complexType name="CustomerList">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="Customers" type="tns:Customer" maxOccurs="unbounded"/>
                    <xsd:element name="NumberOfCustomers" type="xsd:int"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <!--
    Element Name   :  Customer
    Description    :  Represents a customer in the system. Contains all the information related to a specific customer.
    Platform/s Used:  BM
    -->
    <xsd:complexType name="Customer">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="CustomerId" type="xsd:int"/>
                    <xsd:element name="Title" type="xsd:string" />
                    <xsd:element name="FirstName" type="tns:stNonEmptyString" />
                    <xsd:element name="MiddleName" type="xsd:string"/>
                    <xsd:element name="LastName" type="tns:stNonEmptyString"/>
                    <xsd:element name="CustomerStatus" type="xsd:string"/>
                    <xsd:element name="IdentityNumber" type="xsd:string"/>
                    <xsd:element name="IdentityNumberType" type="xsd:string"/>
                    <xsd:element name="NationalIdentityNumber" type="xsd:string"/>
                    <xsd:element name="IsNinVerified" type="xsd:string"/>
                    <xsd:element name="CreatedDateTime" type="xsd:dateTime"/>
                    <xsd:element name="Addresses" type="tns:Address" maxOccurs="unbounded"/>
                    <xsd:element name="DateOfBirth" type="xsd:string"/>
                    <xsd:element name="Version" type="xsd:int"/>
                    <xsd:element name="Gender" type="xsd:string"/>
                    <xsd:element name="Language" type="xsd:string"/>
                    <xsd:element name="EmailAddress" type="xsd:string"/>
                    <xsd:element name="AlternativeContact1" type="xsd:string"/>
                    <xsd:element name="AlternativeContact2" type="xsd:string"/>
                    <xsd:element name="Classification" type="xsd:string"/>
                    <xsd:element name="SecurityGroups" type="xsd:string" maxOccurs="unbounded"/> 
                    <xsd:element name="CustomerSellers" type="xsd:int" maxOccurs="unbounded"/>                   
                    <xsd:element name="SSOIdentity" type="xsd:string"/>
                    <xsd:element name="SSODigest" type="xsd:string"/>
                    <xsd:element name="SSOAuthFlags" type="xsd:int"/>
                    <xsd:element name="OptInLevel" type="xsd:int"/>
                    <xsd:element name="ProductInstances" type="tns:ProductInstance" maxOccurs="unbounded"/>
                    <xsd:element name="CustomerPhotographs" type="tns:Photograph" maxOccurs="unbounded"/>
                    <xsd:element name="AccountManagerCustomerProfileId" type="xsd:int"/>
                    <xsd:element name="OutstandingTermsAndConditions" type="xsd:string" maxOccurs="unbounded"/>
                    <xsd:element name="CustomerRoles" type="tns:CustomerRole" maxOccurs="unbounded"/>
                    <xsd:element name="MothersMaidenName" type="xsd:string"/>
                    <xsd:element name="Nationality" type="xsd:string"/>
                    <xsd:element name="PassportExpiryDate" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="WarehouseId" type="xsd:string"/>
                    <xsd:element name="CreatedByCustomerProfileId" type="xsd:int"/>
                    <xsd:element name="KYCStatus" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ReferralCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CountryCode" type="xsd:string"/>
                    <xsd:element name="MandatoryKYCFields" type="tns:MandatoryKYCFields"/>
                    <xsd:element name="CustomerNinData" type="tns:CustomerNinData"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:simpleType name="stCustomerLookupVerbosity">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="CUSTOMER"/>
            <xsd:enumeration value="CUSTOMER_ADDRESS"/>
            <xsd:enumeration value="CUSTOMER_ADDRESS_PRODUCTS"/>
            <xsd:enumeration value="CUSTOMER_ADDRESS_PRODUCTS_SERVICES"/>
            <xsd:enumeration value="CUSTOMER_ADDRESS_PRODUCTS_SERVICES_SVCAVP"/>
            <xsd:enumeration value="CUSTOMER_PHOTO"/>
            <xsd:enumeration value="CUSTOMER_ADDRESS_CUSTOMERNINDATA"/>
            <xsd:enumeration value="CUSTOMER_PHOTO_ADDRESS_MANDATEKYCFIELD"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- Address -->
    <xsd:complexType name="Address">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="AddressId" type="xsd:int"/>
                    <xsd:element name="Line1" type="xsd:string"/>
                    <xsd:element name="Line2" type="xsd:string"/>
                    <xsd:element name="Zone" type="xsd:string"/>
                    <xsd:element name="Town" type="xsd:string"/>
                    <xsd:element name="State" type="xsd:string"/> 
                    <xsd:element name="Country" type="xsd:string"/>
                    <xsd:element name="Code" type="xsd:string"/>
                    <xsd:element name="Type" type="xsd:string"/>
                    <xsd:element name="PostalMatchesPhysical" type="xsd:boolean"/>
                    <xsd:element name="CustomerId" type="xsd:int"/>
                    <xsd:element name="OrganisationId" type="xsd:int"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="Photograph">
        <xsd:sequence>
            <xsd:element name="PhotoGuid" type="xsd:string"/>
            <xsd:element name="PhotoType" type="xsd:string"/>
            <xsd:element name="Data" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Photograph" type="tns:Photograph"/>
    <xsd:element name="PhotoGuid" type="tns:SCAString"/>
    
    <xsd:complexType name="SSOPasswordResetData">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="GUID" type="xsd:string"/>
                    <xsd:element name="NewSSODigest" type="xsd:string"/>
                </xsd:sequence>       
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="SSOPasswordResetLinkData">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="Identifier" type="xsd:string"/>
                    <xsd:element name="DeliveryMethod" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>       
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="SSOPasswordResetLink">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="GUID" type="xsd:string"/>
                    <xsd:element name="URL" type="xsd:string"/>
                    <xsd:element name="Expiry" type="xsd:dateTime"/>
                </xsd:sequence>       
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="CustomerRole">
        <xsd:sequence>
            <xsd:element name="RoleName" type="xsd:string"></xsd:element>
            <xsd:element name="OrganisationName" type="xsd:string"></xsd:element>
            <xsd:element name="CustomerName" type="xsd:string"></xsd:element>
            <xsd:element name="OrganisationId" type="xsd:int"></xsd:element>
            <xsd:element name="CustomerId" type="xsd:int"></xsd:element>
        </xsd:sequence>
    </xsd:complexType> 
    
    <!-- Organisations -->
    
    <xsd:complexType name="Organisation">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="OrganisationId" type="xsd:int"/>
                    <xsd:element name="OrganisationType" type="xsd:string"/>
                    <xsd:element name="OrganisationSubType" type="xsd:string"/>
                    <xsd:element name="OrganisationName" type="xsd:string"/>
                    <xsd:element name="OrganisationStatus" type="xsd:string"/>
                    <xsd:element name="CreatedDateTime" type="xsd:dateTime"/>
                    <xsd:element name="Version" type="xsd:int"/>
                    <xsd:element name="Addresses" type="tns:Address" maxOccurs="unbounded"/>
                    <xsd:element name="EmailAddress" type="xsd:string"/>
                    <xsd:element name="AlternativeContact1" type="xsd:string"/>
                    <xsd:element name="AlternativeContact2" type="xsd:string"/>
                    <xsd:element name="Industry" type="xsd:string"/>
                    <xsd:element name="Size" type="xsd:string"/>
                    <xsd:element name="TaxNumber" type="xsd:string"/>
                    <xsd:element name="CompanyNumber" type="xsd:string"/>
                    <xsd:element name="CreditAccountNumber" type="xsd:string"/>
                    <xsd:element name="ChannelCode" type="xsd:string"/>
                    <xsd:element name="ModificationRoles" type="xsd:string"  maxOccurs="unbounded"/>
                    <xsd:element name="OrganisationPhotographs" type="tns:Photograph" maxOccurs="unbounded"/>
                    <xsd:element name="AccountManagerCustomerProfileId" type="xsd:int"/>
                    <xsd:element name="CustomerRoles" type="tns:CustomerRole" maxOccurs="unbounded"/>
                    <xsd:element name="OrganisationSellers" type="xsd:int"  maxOccurs="unbounded"/>
                    <xsd:element name="CreatedByCustomerProfileId" type="xsd:int"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="OrganisationList">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="Organisations" type="tns:Organisation" maxOccurs="unbounded"/>
                    <xsd:element name="NumberOfOrganisations" type="xsd:int"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="OrganisationQuery">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="OrganisationId" type="xsd:int"/>
                    <xsd:element name="CustomerId" type="xsd:int"/>
                    <xsd:element name="OrganisationName" type="xsd:string"/>
                    <xsd:element name="ResultLimit" type="xsd:int"/>
                    <xsd:element name="RolesOffset" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="RolesResultLimit" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="Verbosity" type="tns:stOrganisationLookupVerbosity"/>
                </xsd:sequence>       
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:simpleType name="stOrganisationLookupVerbosity">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="MAIN"/>
            <xsd:enumeration value="MAIN_ROLES"/>
            <xsd:enumeration value="MAIN_PHOTO"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:complexType name="VerifyDefacedCustomerRequest">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="nin" type="xsd:string"/>
                    <xsd:element name="questionCode" type="xsd:string" />
                    <xsd:element name="answer" type="xsd:string" />
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="VerifyKYCMandatoryFieldsRequest">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="titleVerified" type="xsd:string"/>
                    <xsd:element name="nameVerified" type="xsd:string" />
                    <xsd:element name="mobileVerified" type="xsd:string" />
                    <xsd:element name="emailVerified" type="xsd:string" />
                    <xsd:element name="genderVerified" type="xsd:string" />
                    <xsd:element name="dobVerified" type="xsd:string"/>
                    <xsd:element name="nationalityVerified" type="xsd:string" />
                    <xsd:element name="physicalAddressVerified" type="xsd:string" />
                    <xsd:element name="facialPitureVerified" type="xsd:string" />
                    <xsd:element name="validIdCardVerified" type="xsd:string" />
                    <xsd:element name="fingerPrintVerified" type="xsd:string" />
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:element name="VerifyKYCMandatoryFieldsRequest" type="tns:VerifyKYCMandatoryFieldsRequest"/>
    <xsd:element name="VerifyDefacedCustomerRequest" type="tns:VerifyDefacedCustomerRequest"/>
    <xsd:element name="Customer" type="tns:Customer"/>
    <xsd:element name="CustomerId" type="tns:SCAInteger"/>
    <xsd:element name="CustomerList" type="tns:CustomerList"/>
    <xsd:element name="CustomerQuery" type="tns:CustomerQuery"/>
    <xsd:element name="ModifiedCustomer" type="tns:Customer"/>
    <xsd:element name="NewCustomer" type="tns:Customer"/>
    <xsd:element name="NewAddress" type="tns:Address"/>
    <xsd:element name="Address" type="tns:Address"/>
    <xsd:element name="ModifiedAddress" type="tns:Address"/>
    <xsd:element name="AddressToDelete" type="tns:Address"/>
    <xsd:element name="SSOPasswordResetData" type="tns:SSOPasswordResetData"/>
    <xsd:element name="SSOPasswordResetLink" type="tns:SSOPasswordResetLink"/>
    <xsd:element name="SSOPasswordResetLinkData" type="tns:SSOPasswordResetLinkData"/>
    <xsd:element name="OrganisationQuery" type="tns:OrganisationQuery"/>
    <xsd:element name="Organisation" type="tns:Organisation"/>
    <xsd:element name="OrganisationId" type="tns:SCAInteger"/>
    <xsd:element name="NewOrganisation" type="tns:Organisation"/>
    <xsd:element name="ModifiedOrganisation" type="tns:Organisation"/>
    <xsd:element name="OrganisationList" type="tns:OrganisationList"/>
    
    <!--
    #################################################
    Authentication
    #################################################
    -->

    <!--
    Element Name   : AuthenticationQuery
    Description    : It is used to authenticate a user into the system during the logon process.
    Platform/s Used: IM
    -->
    <xsd:complexType name="AuthenticationQuery">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="SSOIdentity" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SSOEncryptedPassword" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="IMSPublicIdentity" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ActivationCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="EncryptedAppKey" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="AuthenticationQuery" type="tns:AuthenticationQuery"></xsd:element>

    <!--
    Element Name   : AuthenticationResult
    Description    : Returns the status of authenticating a user into the system. If a user was
                     was successfully authenticated, a list of groups that the user belongs to
                     is returned.
    Platform/s Used: IM
    -->
    <xsd:complexType name="AuthenticationResult">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="Done" type="tns:stDone"></xsd:element>
                    <xsd:element name="SecurityGroups" type="xsd:string" maxOccurs="unbounded" minOccurs="0"></xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="AuthenticationResult" type="tns:AuthenticationResult"></xsd:element>


    <!--
    #################################################
    Product and Service Management
    #################################################
    -->

    <!--
    Element Name   : ProductSpecification
    Description    : A detailed descroption of the product to be offered externally to SMILE customers.
    Platform/s Used: PC (Product Catalogue) platform.
    -->
    <xsd:complexType name="ProductSpecification">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="ProductSpecificationId" type="xsd:int"/>
                    <xsd:element name="Name" type="xsd:string"/>
                    <xsd:element name="Description" type="xsd:string"/>
                    <xsd:element name="AvailableFrom" type="xsd:dateTime"/>
                    <xsd:element name="AvailableTo" type="xsd:dateTime"/>
                    <xsd:element name="ProvisionRoles" type="xsd:string" />
                    <xsd:element name="Segments" type="xsd:string" />
                    <xsd:element name="ProductServiceSpecificationMappings" type="tns:ProductServiceSpecificationMapping" maxOccurs="unbounded" />
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="ProductSpecificationList">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="ProductSpecifications" type="tns:ProductSpecification" maxOccurs="unbounded"/>
                    <xsd:element name="NumberOfProductSpecifications" type="xsd:int"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="UnitCreditSpecificationList">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="UnitCreditSpecifications" type="tns:UnitCreditSpecification" maxOccurs="unbounded"/>
                    <xsd:element name="NumberOfUnitCreditSpecifications" type="xsd:int"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <!--
    Element Name   : ServiceSpecification
    Description    : Describes the invariant characteristics, features and behaviour of Service entities.
                     Serves as a template from which multiple service entities can be created.
    Platform/s Used: PC
    -->

    <xsd:complexType name="ServiceSpecification">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="ServiceSpecificationId" type="xsd:int"/>
                    <xsd:element name="Name" type="xsd:string"/>
                    <xsd:element name="Description" type="xsd:string"/>
                    <xsd:element name="ServiceCode" type="xsd:string"/>
                    <xsd:element name="AVPs" type="tns:AVP" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>


    
    <!--
    Element Name   : RatePlan
    Description    : 
    Platform/s Used: ?
    -->
    <xsd:complexType name="RatePlan">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="RatePlanId" type="xsd:int"/>
                    <xsd:element name="Name" type="xsd:string"/>
                    <xsd:element name="Description" type="xsd:string"/>
                    <xsd:element name="RatingEngineClass" type="xsd:string"/>
                    <xsd:element name="EventBased" type="xsd:boolean"/>
                    <xsd:element name="SessionBased" type="xsd:boolean"/>
                    <xsd:element name="AVPs" type="tns:AVP" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <!--
    Element Name   : ProductInstance
    Description    :
    Platform/s Used: ?
    -->
    <xsd:complexType name="ProductInstance">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="ProductInstanceId" type="xsd:int"/>
                    <xsd:element name="ProductSpecificationId" type="xsd:int"/>
                    <xsd:element name="CustomerId" type="xsd:int"/>
                    <xsd:element name="OrganisationId" type="xsd:int"/>
                    <xsd:element name="Segment" type="xsd:string"/>
                    <xsd:element name="CreatedDateTime" type="xsd:dateTime" minOccurs="0"/>
                    <xsd:element name="ProductServiceInstanceMappings" type="tns:ProductServiceInstanceMapping" maxOccurs="unbounded" />
                    <xsd:element name="PromotionCode" type="xsd:string"/>
                    <xsd:element name="FriendlyName" type="xsd:string"/>
                    <xsd:element name="LogicalId" type="xsd:int"/>
                    <xsd:element name="PhysicalId" type="xsd:string"/>     
                    <xsd:element name="CreatedByOrganisationId" type="xsd:int"/>
                    <xsd:element name="Campaigns" type="tns:CampaignData" maxOccurs="unbounded"/>
                    <xsd:element name="LastDevice" type="xsd:string"/>
                    <xsd:element name="ReferralCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="FirstActivityDateTime" type="xsd:dateTime" minOccurs="0"/>
                    <xsd:element name="LastActivityDateTime" type="xsd:dateTime" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <!--
    Element Name   : ServiceInstance
    Description    :
    Platform/s Used: ?
    -->
    <xsd:complexType name="ServiceInstance">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="ServiceInstanceId" type="xsd:int"/>
                    <xsd:element name="ServiceSpecificationId" type="xsd:int"/>
                    <xsd:element name="ProductInstanceId" type="xsd:int"/>
                    <xsd:element name="CustomerId" type="xsd:int"/>
                    <xsd:element name="AccountId" type="xsd:long"/>
                    <xsd:element name="Status" type="xsd:string"/>
                    <xsd:element name="CreatedDateTime" type="xsd:dateTime" minOccurs="0"/>
                    <xsd:element name="RatePlanId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="AVPs" type="tns:AVP" maxOccurs="unbounded" minOccurs="0"/>
                    <xsd:element name="ServiceInstanceMappings" type="tns:ServiceInstanceMapping" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="ServiceInstanceMapping">
        <xsd:sequence>
            <xsd:element name="Identifier" type="xsd:string"></xsd:element>
            <xsd:element name="IdentifierType" type="xsd:string"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="ServiceInstanceList">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="ServiceInstances" type="tns:ServiceInstance" maxOccurs="unbounded"/>
                    <xsd:element name="NumberOfServiceInstances" type="xsd:int"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="ProductInstanceList">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="ProductInstances" type="tns:ProductInstance" maxOccurs="unbounded"/>
                    <xsd:element name="NumberOfProductInstances" type="xsd:int"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="ProductServiceInstanceMapping">
        <xsd:sequence>
            <xsd:element name="ServiceInstance" type="tns:ServiceInstance"/>
            <xsd:element name="RatePlanId" type="xsd:int"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ProductServiceSpecificationMapping">
        <xsd:sequence>
            <xsd:element name="ServiceSpecification" type="tns:ServiceSpecification"/>
            <xsd:element name="RatePlanId" type="xsd:int"/>
            <xsd:element name="MinServiceOccurences" type="xsd:int"/>
            <xsd:element name="MaxServiceOccurences" type="xsd:int"/>
            <xsd:element name="GroupId" type="xsd:int"/>
            <xsd:element name="ProvisionRoles" type="xsd:string"/>
            <xsd:element name="AvailableFrom" type="xsd:dateTime"/>
            <xsd:element name="AvailableTo" type="xsd:dateTime"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="UnitCreditSpecification">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="UnitCreditSpecificationId" type="xsd:int"/>
                    <xsd:element name="Name" type="xsd:string"/>
                    <xsd:element name="ItemNumber" type="xsd:string"/>
                    <xsd:element name="ProductServiceMappings" type="tns:ProductServiceMapping" maxOccurs="unbounded" />
                    <xsd:element name="PriceInCents" type="xsd:double"/>
                    <xsd:element name="ValidityDays" type="xsd:int"/>
                    <xsd:element name="UsableDays" type="xsd:int"/>
                    <xsd:element name="AvailableFrom" type="xsd:dateTime"/>
                    <xsd:element name="AvailableTo" type="xsd:dateTime"/>
                    <xsd:element name="Units" type="xsd:double"/>
                    <xsd:element name="UnitType" type="xsd:string" />
                    <xsd:element name="Description" type="xsd:string" />
                    <xsd:element name="PurchaseRoles" type="xsd:string" />
                    <xsd:element name="Priority" type="xsd:int"/>
                    <xsd:element name="FilterClass" type="xsd:string"/>
                    <xsd:element name="WrapperClass" type="xsd:string"/>
                    <xsd:element name="Configuration" type="xsd:string"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="ProductServiceMapping">
        <xsd:sequence>
            <xsd:element name="ServiceSpecificationId" type="xsd:int"/>
            <xsd:element name="ProductSpecificationId" type="xsd:int"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="UnitCreditInstance">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="UnitCreditInstanceId" type="xsd:int"/>
                    <xsd:element name="Name" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="UnitType" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="UnitCreditSpecificationId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="AccountId" type="xsd:long" minOccurs="0"/>
                    <xsd:element name="PurchaseDate" type="xsd:dateTime" minOccurs="0"/>
                    <xsd:element name="StartDate" type="xsd:dateTime" minOccurs="0"/>
                    <xsd:element name="EndDate" type="xsd:dateTime" minOccurs="0"/>
                    <xsd:element name="ExpiryDate" type="xsd:dateTime" minOccurs="0"/>
                    <xsd:element name="CurrentUnitsRemaining" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="AvailableUnitsRemaining" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="UnitsAtStart" type="xsd:double" minOccurs="0"/>
                    <xsd:element name="ProductInstanceId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="ExtTxId" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Info" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SaleLineId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="Comment" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="ProductOrder">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="ProductSpecificationId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="ProductInstanceId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="CustomerId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="OrganisationId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="Segment" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Action" type="tns:stAction" minOccurs="0"/>
                    <xsd:element name="AVPs" type="tns:AVP" maxOccurs="unbounded" minOccurs="0"/>
                    <xsd:element name="ServiceInstanceOrders" type="tns:ServiceInstanceOrder" minOccurs="0" maxOccurs="unbounded"/>
                    <xsd:element name="PromotionCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ReferralCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="FriendlyName" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AllowPendingSIMSale" type="xsd:boolean"/>
                    <xsd:element name="KitItemNumber" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="DeviceSerialNumber" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="ServiceInstanceOrder">
        <xsd:sequence>
            <xsd:element name="ServiceInstance" type="tns:ServiceInstance"/>
            <xsd:element name="Action" type="tns:stAction"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="ServiceInstanceQuery">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="ServiceInstanceId" type="xsd:int" minOccurs="0"></xsd:element>
                    <xsd:element name="Identifier" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="IdentifierType" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="ProductInstanceId" minOccurs="0" type="xsd:int"/>
                    <xsd:element name="CustomerId" minOccurs="0" type="xsd:int"/>
                    <xsd:element name="ServiceSpecificationId" minOccurs="0" type="xsd:int"/>
                    <xsd:element name="AccountId" type="xsd:long" minOccurs="0"></xsd:element>
                    <xsd:element name="IPAddress" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Verbosity" type="tns:stServiceInstanceLookupVerbosity"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="ProductInstanceQuery">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="ServiceInstanceId" minOccurs="0" type="xsd:int"/>
                    <xsd:element name="ProductInstanceId" minOccurs="0" type="xsd:int"/>
                    <xsd:element name="CustomerId" type="xsd:int"/>
                    <xsd:element name="OrganisationId" type="xsd:int"/>
                    <xsd:element name="PhysicalId" minOccurs="0" type="xsd:string"/>
                    <xsd:element name="ProductSpecificationId" minOccurs="0" type="xsd:int"/>
                    <xsd:element name="Verbosity" type="tns:stProductInstanceLookupVerbosity"/>
                    <xsd:element name="Offset" minOccurs="0" type="xsd:int"/>
                    <xsd:element name="ResultLimit" minOccurs="0" type="xsd:int"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:simpleType name="stServiceInstanceLookupVerbosity">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="MAIN"/>
            <xsd:enumeration value="MAIN_SVCAVP"/>
            <xsd:enumeration value="MAIN_SVCAVP_STATIC_ONLY"/>
            <xsd:enumeration value="MAIN_SVCAVP_MAPPINGS"/>
            <xsd:enumeration value="MAIN_SVCAVP_PHOTO"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="stProductInstanceLookupVerbosity">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="MAIN"/>
            <xsd:enumeration value="MAIN_CAMPAIGNS_CAMPAIGNUC"/>
            <xsd:enumeration value="MAIN_SVC"/>
            <xsd:enumeration value="MAIN_SVC_CAMPAIGNS"/>
            <xsd:enumeration value="MAIN_SVC_CAMPAIGNS_CAMPAIGNUC"/>
            <xsd:enumeration value="MAIN_SVC_SVCAVP"/>
            <xsd:enumeration value="MAIN_SVC_SVCAVP_CAMPAIGNS"/>
            <xsd:enumeration value="MAIN_SVC_SVCAVP_CAMPAIGNS_CAMPAIGNUC"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:complexType name="ProductSpecificationQuery">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="ProductSpecificationId" type="xsd:int"/>
                    <xsd:element name="Verbosity" type="tns:stProductSpecificationLookupVerbosity"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:simpleType name="stProductSpecificationLookupVerbosity">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="MAIN"/> <!-- include product spec only-->
            <xsd:enumeration value="MAIN_PRODAVP"/> <!-- include product spec AVP-->
            <xsd:enumeration value="MAIN_PRODAVP_SVC"/> <!-- Include service specifications -->
            <xsd:enumeration value="MAIN_PRODAVP_SVC_SVCAVP"/> <!-- include service spec AVP-->
        </xsd:restriction>
    </xsd:simpleType>
        
    <xsd:complexType name="ServiceSpecificationQuery">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="ServiceSpecificationId" type="xsd:int"/>
                    <xsd:element name="Verbosity" type="tns:stServiceSpecificationLookupVerbosity"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:simpleType name="stServiceSpecificationLookupVerbosity">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="MAIN"/> <!-- include service spec AVP-->
            <xsd:enumeration value="MAIN_SVCAVP"/> <!-- include service spec avps-->
        </xsd:restriction>
    </xsd:simpleType>
    
    
    <xsd:complexType name="UnitCreditSpecificationQuery">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="UnitCreditSpecificationId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="ItemNumber" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Verbosity" type="tns:stUnitCreditSpecificationLookupVerbosity"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:simpleType name="stUnitCreditSpecificationLookupVerbosity">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="MAIN"/>
            <xsd:enumeration value="MAIN_SVCSPECIDS"/>
        </xsd:restriction>
    </xsd:simpleType>
    

    <xsd:element name="ProductInstanceQuery" type="tns:ProductInstanceQuery"></xsd:element>
    <xsd:element name="ProductInstanceList" type="tns:ProductInstanceList"></xsd:element>
    <xsd:element name="ProductInstance" type="tns:ProductInstance"></xsd:element>
    <xsd:element name="ProductSpecification" type="tns:ProductSpecification"></xsd:element>
    <xsd:element name="UnitCreditSpecificationQuery" type="tns:UnitCreditSpecificationQuery"></xsd:element>
    <xsd:element name="ProductSpecificationList" type="tns:ProductSpecificationList"></xsd:element>
    <xsd:element name="UnitCreditSpecificationList" type="tns:UnitCreditSpecificationList"></xsd:element>
    <xsd:element name="ServiceSpecificationQuery" type="tns:ServiceSpecificationQuery"></xsd:element>
    <xsd:element name="ProductSpecificationQuery" type="tns:ProductSpecificationQuery"></xsd:element>
    <xsd:element name="RatePlanId" type="tns:SCAInteger"></xsd:element>
    <xsd:element name="ProductOrder" type="tns:ProductOrder"></xsd:element>
    <xsd:element name="ServiceInstanceQuery" type="tns:ServiceInstanceQuery"></xsd:element>
    <xsd:element name="ServiceInstance" type="tns:ServiceInstance"></xsd:element>
    <xsd:element name="ServiceInstanceList" type="tns:ServiceInstanceList"></xsd:element>
    <xsd:element name="RatePlan" type="tns:RatePlan"></xsd:element>
    <xsd:element name="ServiceSpecification" type="tns:ServiceSpecification"></xsd:element>
    
    <!--
    #################################################
    Main SCA Types
    #################################################
    -->
    <xsd:simpleType name="stAction">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="create"/>
            <xsd:enumeration value="update"/>
            <xsd:enumeration value="delete"/>
            <xsd:enumeration value="none"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="stDone">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="true"/>
            <xsd:enumeration value="false"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="stNonEmptyString">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="SCAString">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="String" type="xsd:string"></xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="SCAInteger">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="Integer" type="xsd:int"></xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="SCADateTime">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="DateTime" type="xsd:dateTime"></xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="Done">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="Done" type="tns:stDone"></xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="SCAError">
        <xsd:sequence>
            <xsd:element name="ErrorDesc" type="xsd:string" />
            <xsd:element name="ErrorType" type="xsd:string"/>
            <xsd:element name="ErrorCode" type="xsd:string" />
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SCAContext">
        <xsd:sequence>
            <xsd:element name="Method" type="xsd:string" minOccurs="0"></xsd:element>
            <xsd:element name="TxId" type="xsd:string"></xsd:element>
            <xsd:element name="OriginatingIdentity" type="xsd:string"></xsd:element>
            <xsd:element name="OriginatingIP" type="xsd:string"></xsd:element>
            <xsd:element name="Async" type="xsd:boolean" minOccurs="0"></xsd:element>
            <xsd:element name="Roles" type="xsd:string" minOccurs="0" maxOccurs="unbounded"></xsd:element>
            <xsd:element name="Obviscated" type="xsd:string" minOccurs="0"></xsd:element>
            <xsd:element name="Tenant" type="xsd:string" minOccurs="0"></xsd:element>
            <xsd:element name="Comment" type="xsd:string" minOccurs="0"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SCAObject">
        <xsd:sequence>
            <xsd:element name="SCAContext" type="tns:SCAContext" minOccurs="0"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="PlatformEndPoint">
        <xsd:sequence>
            <xsd:element name="Address" type="xsd:string"></xsd:element>
            <xsd:element name="PlatformName" type="xsd:string"></xsd:element>
            <xsd:element name="Status" type="xsd:string"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="PlatformEndPointList">
        <xsd:sequence>
            <xsd:element name="PlatformEndPoints" type="xsd:string"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="AVP">
        <xsd:sequence>
            <xsd:element name="Attribute" type="xsd:string"/>
            <xsd:element name="Value" type="xsd:string"/>
            <xsd:element name="UserDefined" type="xsd:boolean" minOccurs="0"/>
            <xsd:element name="ValidationRule" type="xsd:string" minOccurs="0"/>
            <xsd:element name="InputType" type="xsd:string" minOccurs="0"/>
            <xsd:element name="TechnicalDescription" type="xsd:string" minOccurs="0"/>
            <xsd:element name="ProvisionRoles" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    

    <xsd:element name="PlatformEndPointList" type="tns:PlatformEndPointList"></xsd:element>
    <xsd:element name="SCAObject" type="tns:SCAObject"></xsd:element>
    <xsd:element name="SCAError" type="tns:SCAError"></xsd:element>
    <xsd:element name="Test" type="tns:SCAString"></xsd:element>
    <xsd:element name="AVP" type="tns:AVP"></xsd:element>
    <xsd:element name="Done" type="tns:Done"></xsd:element>
    <xsd:element name="SCADateTime" type="tns:SCADateTime"></xsd:element>
    <xsd:element name="SCAInteger" type="tns:SCAInteger"></xsd:element>
    <xsd:element name="SCAString" type="tns:SCAString"></xsd:element>

    <!--
    #################################################
    Messaging
    #################################################
    -->
    
    <xsd:complexType name="ShortMessage">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="From" type="xsd:string"></xsd:element>
                    <xsd:element name="To" type="xsd:string"></xsd:element>
                    <xsd:element name="Body" type="xsd:string"></xsd:element>
                    <xsd:element name="DataCodingScheme" type="xsd:byte"></xsd:element>
                    <xsd:element name="CampaignId" type="xsd:string" minOccurs="0"></xsd:element>
                    <xsd:element name="Priority" type="tns:stMessagePriority" minOccurs="0"></xsd:element>
                    <xsd:element name="ValiditySeconds" type="xsd:int" minOccurs="0"></xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:element name="ShortMessage" type="tns:ShortMessage"></xsd:element>
    <xsd:element name="NewShortMessage" type="tns:ShortMessage"></xsd:element>

    <xsd:simpleType name="stMessagePriority">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="HIGH"/>
            <xsd:enumeration value="MEDIUM"/>
            <xsd:enumeration value="LOW"/>
        </xsd:restriction>
    </xsd:simpleType>
    <!--
    #################################################
    Account Management
    #################################################
    -->

    <xsd:complexType name="Account">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="AccountId" type="xsd:long"/>
                    <xsd:element name="Status" minOccurs="0" type="xsd:int"/>
                    <xsd:element name="CurrentBalanceInCents" type="xsd:double"></xsd:element>
                    <xsd:element name="AvailableBalanceInCents" type="xsd:double"></xsd:element>
                    <xsd:element name="UnitCreditInstances" type="tns:UnitCreditInstance" minOccurs="0" maxOccurs="unbounded"></xsd:element>
                    <xsd:element name="ServiceInstances" type="tns:ServiceInstance" minOccurs="0" maxOccurs="unbounded"/>
                    <xsd:element name="Reservations" type="tns:Reservation" minOccurs="0"  maxOccurs="unbounded"></xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    

    <xsd:complexType name="Reservation">
        <xsd:sequence>
            <xsd:element name="AccountId" type="xsd:long"/>
            <xsd:element name="UnitCreditInstanceId" type="xsd:int"/>
            <xsd:element name="SessionId" type="xsd:string"/>
            <xsd:element name="ExpiryDate" type="xsd:dateTime"></xsd:element>
            <xsd:element name="ReservationDate" type="xsd:dateTime"></xsd:element>
            <xsd:element name="UnitCreditUnits" type="xsd:double"></xsd:element>
            <xsd:element name="AmountInCents" type="xsd:double"></xsd:element>
            <xsd:element name="Description" type="xsd:string" minOccurs="0"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="ProvisionUnitCreditLine">
        <xsd:sequence>
            <xsd:element name="AccountId" type="xsd:long"></xsd:element>
            <xsd:element name="UnitCreditSpecificationId" type="xsd:int"></xsd:element>
            <xsd:element name="UnitCreditName" type="xsd:string" minOccurs="0"/>
            <xsd:element name="ProductInstanceId" type="xsd:int"></xsd:element>
            <xsd:element name="NumberToProvision" type="xsd:int"></xsd:element>
            <xsd:element name="DaysGapBetweenStart" type="xsd:int"></xsd:element>
            <xsd:element name="POSCentsPaidEach" type="xsd:double"/>
            <xsd:element name="POSCentsDiscountEach" type="xsd:double"/>
            <xsd:element name="ItemNumber" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="SplitUnitCreditData">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="UnitCreditInstanceId" type="xsd:int"/>
                    <xsd:element name="TargetAccountId" type="xsd:long"/>
                    <xsd:element name="TargetProductInstanceId" type="xsd:int"/>
                    <xsd:element name="Units" type="xsd:double"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    
    <xsd:complexType name="BalanceTransferLine">
        <xsd:sequence>
            <xsd:element name="SourceAccountId" type="xsd:long"/>
            <xsd:element name="TargetAccountId" type="xsd:long"/>
            <xsd:element name="AmountInCents" type="xsd:double"/>
            <xsd:element name="SaleId" type="xsd:int" minOccurs="0" />
            <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
            <xsd:element name="PaymentTransactionData" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="BalanceTransferData">
        <xsd:complexContent>
            <xsd:extension base="tns:SCAObject">
                <xsd:sequence>
                    <xsd:element name="SourceAccountId" type="xsd:long"/>
                    <xsd:element name="TargetAccountId" type="xsd:long"/>
                    <xsd:element name="AmountInCents" type="xsd:double"/>
                    <xsd:element name="SaleId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PaymentTransactionData" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="AdditionalTransferLines" type="tns:BalanceTransferLine" maxOccurs="unbounded" minOccurs="0"></xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="AccountList">
        <xsd:sequence>
